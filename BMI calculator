weight=float(input("Enter your weight kg:"))
height=float(input("Enter your height in meter:"))
BMI=weight/(height**2)
print("Your Body mass Index is:",weight/(height**2))
if(BMI<18.5):
    print("Underweight")
elif(18.5<BMI<24.9):
    print("Normal weight")
elif(25<BMI<29.9):
    print("overweight")
elif(30<BMI<34.9):
    print("Obesity class I")
elif(35<BMI<39.9):
    print("Obesity class II")
else:
    print("Obesity class III")
